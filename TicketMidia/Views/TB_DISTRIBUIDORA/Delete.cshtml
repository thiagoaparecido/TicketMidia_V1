@model TicketMidia.Models.TB_DISTRIBUIDORA

<h3>Exclusão</h3>

<h3>Tem certeza que deseja excluir esta Distribuidora?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            CNPJ
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DIS_CNPJ)
        </dd>
        <dt>
            Distribuidora
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DIS_NM)
        </dd>
        <dt>
            Inclusão
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DIS_DT_INC)
        </dd>

        <dt>
            Alteração
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DIS_DT_ALT)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color" style="text-align:right">
            <a href="/TB_DISTRIBUIDORA" class="btn btn-default btn-md" role="button">Voltar</a>
            <input type="submit" value="Excluir" id="Salvar" class="btn btn-danger btn-md" />
        </div>
    }
</div>
